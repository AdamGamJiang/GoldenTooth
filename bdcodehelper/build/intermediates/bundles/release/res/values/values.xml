<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="bg_gray">#f1f1f1</color>
    <color name="bg_gray_pressed">#e8e8e8</color>
    <color name="bg_position_bar_down">#1A8BC34A</color>
    <color name="bg_position_bar_up">#0D8BC34A</color>
    <color name="brown">#d54c1c</color>
    <color name="check_bg">#f1f1f1</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#8BC34A</color>
    <color name="colorPrimaryDark">#76A63E</color>
    <color name="colorPrimaryLight">#E8F3BD</color>
    <color name="colorSecondary">#ff9900</color>
    <color name="colorSecondaryDark">#d88200</color>
    <color name="divider_gray">#cbcbcb</color>
    <color name="drop_down_selected">#8BC34A</color>
    <color name="drop_down_unselected">#808080</color>
    <color name="gray">#cccccc</color>
    <color name="mask_color">#88888888</color>
    <color name="province_line_border">#C7C7C7</color>
    <color name="txt_black">#212121</color>
    <color name="txt_gray">#808080</color>
    <color name="txt_light_gray">#CCCCCC</color>
    <color name="txt_link_blue">#1ea5ff</color>
    <color name="un_press_color">#8BC34A</color>
    <color name="white">#ffffff</color>
    <declare-styleable name="BottomTabRadioButton"><attr format="enum" name="hint_mode"><enum name="none" value="0"/><enum name="point" value="1"/><enum name="num" value="2"/></attr><attr format="color" name="hint_color"/><attr format="dimension" name="hint_toppadding"/><attr format="dimension" name="hint_rightpadding"/><attr format="dimension" name="point_radius"/><attr format="integer" name="num"/><attr format="dimension" name="num_radius"/><attr format="dimension" name="num_size"/><attr format="color" name="num_color"/></declare-styleable>
    <declare-styleable name="DrawableTextView"><attr format="dimension" name="drawableWidth"/><attr format="dimension" name="drawableHeight"/></declare-styleable>
    <declare-styleable name="DropDownMenu"><attr format="color" name="ddunderlineColor"/><attr format="color" name="dddividerColor"/><attr format="color" name="ddtextSelectedColor"/><attr format="color" name="ddtextUnselectedColor"/><attr format="color" name="ddmenuBackgroundColor"/><attr format="color" name="ddmaskColor"/><attr format="dimension" name="ddmenuTextSize"/><attr format="reference" name="ddmenuSelectedIcon"/><attr format="reference" name="ddmenuUnselectedIcon"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="banner_height">136dp</dimen>
    <dimen name="corner_radius">2dp</dimen>
    <dimen name="margin_huge">64dp</dimen>
    <dimen name="margin_large">32dp</dimen>
    <dimen name="margin_medium">16dp</dimen>
    <dimen name="margin_only_txt">48dp</dimen>
    <dimen name="margin_small">8dp</dimen>
    <dimen name="margin_tiny">4dp</dimen>
    <dimen name="txtsize_body">14sp</dimen>
    <dimen name="txtsize_caption">12sp</dimen>
    <dimen name="txtsize_display1">34sp</dimen>
    <dimen name="txtsize_headline">24sp</dimen>
    <dimen name="txtsize_subhead">16sp</dimen>
    <dimen name="txtsize_title">20sp</dimen>
    <string name="dialog_negative">取消</string>
    <string name="dialog_positive">确定</string>
    <style name="AbsListViewCommon" parent="Widget.AppCompat.ListView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">

    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="android:windowBackground">@color/bg_gray</item>
    </style>
    <style name="ButtonPrimary" parent="TextViewSubhead">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/txt_white2gray_sel</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/correct_primary2dark_sel</item>
    </style>
    <style name="EditTextSingleLine" parent="@style/Widget.AppCompat.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColorHint">@color/txt_light_gray</item>
        <item name="android:textColor">@color/txt_black</item>
        <item name="android:textSize">@dimen/txtsize_subhead</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="GridViewImages" parent="AbsListViewCommon">
        <item name="android:horizontalSpacing">4dp</item>
        <item name="android:verticalSpacing">4dp</item>
    </style>
    <style name="ListViewLineDivider" parent="AbsListViewCommon">
        <item name="android:divider">@color/divider_gray</item>
        <item name="android:dividerHeight">1px</item>
    </style>
    <style name="RadioButtonBottomTab">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/bg_white2gray_sel</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">@dimen/margin_small</item>
        <item name="android:paddingTop">@dimen/margin_small</item>
    </style>
    <style name="TextViewBody" parent="TextAppearance.AppCompat.Body1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/txt_black</item>
    </style>
    <style name="TextViewCaption" parent="TextAppearance.AppCompat.Caption">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/txt_light_gray</item>
    </style>
    <style name="TextViewHeadline" parent="TextAppearance.AppCompat.Headline">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/txt_black</item>
    </style>
    <style name="TextViewSubhead" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/txt_black</item>
    </style>
    <style name="TextViewTitle" parent="TextAppearance.AppCompat.Title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/txt_black</item>
    </style>
    <style name="custom_dialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>

    </style>
</resources>